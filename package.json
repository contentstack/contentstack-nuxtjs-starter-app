{
  "name": "contentstack-nuxtjs-starter-app",
  "description": "A simple starter app build using NuxtJS and Contentstack",
  "version": "1.0.1",
  "private": true,
  "author": "Contentstack",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/robots": "^2.5.0",
    "json-viewer-js": "^1.0.7",
    "moment": "^2.29.1",
    "vue-fragment": "^1.5.2",
    "vue-json-viewer": "^2.2.19"
  },
  "devDependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "async": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "contentstack": "^3.12.2",
    "core-js": "^3.9.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^11.2.3",
    "nuxt": "^2.15.2",
    "prettier": "^2.1.2"
  }
}
